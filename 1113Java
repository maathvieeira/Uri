import java.util.*;

public class Main {

    
    public static void main(String[] args) {
        Scanner teclado = new Scanner(System.in);
        
        while(true){
            
            int N = teclado.nextInt(); int N2 = teclado.nextInt();
            
            if(N==N2){
                break;
            }
            if(N>N2){
             System.out.printf("Decrescente\n");
         } else if (N2>N){
             System.out.printf("Crescente\n");
            }
        }
    }
    
}
