import java.util.*;

public class Main {

    
    public static void main(String[] args) {
        Scanner teclado = new Scanner(System.in);
        
        int n = teclado.nextInt();
        
        
        for(int i=2; i<=n; i=i+2){
            int conta = i*i;
            System.out.println(i+"^2 = "+conta);
        }
    }
    
}
