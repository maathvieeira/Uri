import java.util.*;

public class Main {

    
    public static void main(String[] args) {
        Scanner entrada = new Scanner(System.in);
        
        int i, cha, m, n = 0;
        
        cha = entrada.nextInt();
        
        for(i=0; i<5; i++){
            m = entrada.nextInt();
            if(m==cha) n++; 
        }
        
        System.out.printf("%d\n",n);
    }
    
}
