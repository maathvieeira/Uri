import java.util.*;

public class Main {

    
    public static void main(String[] args) {
       Scanner teclado = new Scanner(System.in);
       
       int N = teclado.nextInt();
       
       
       for(int i=2; i<10000; i=i+N){
           int resto = i%N;
           if(resto==2){
           System.out.println(i);
           }
       }
    }
    
}
